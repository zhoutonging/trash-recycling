<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mengzhou.trashrecycling.mapper.CabinetlocationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mengzhou.trashrecycling.model.Cabinetlocation">
        <id column="id" property="id" />
        <result column="cabinetName" property="cabinetName" />
        <result column="lat" property="lat" />
        <result column="lon" property="lon" />
        <result column="createTime" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cabinetName AS cabinetName, lat, lon, createTime AS createTime
    </sql>

</mapper>
